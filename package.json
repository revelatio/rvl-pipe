{
  "name": "rvl-pipe",
  "version": "2.0.0",
  "description": "Functions for easier async pipelining/composing of promises",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "rollup -c",
    "lint": "standard",
    "format": "prettier-standard --format"
  },
  "keywords": [
    "pipe",
    "promises",
    "async"
  ],
  "files": [
    "build"
  ],
  "repository": "https://github.com/revelatio/rvl-pipe",
  "author": "Ernesto Freyre",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "coveralls": "^3.0.9",
    "jest": "^24.9.0",
    "nyc": "^15.0.0",
    "prettier-standard": "^16.1.0",
    "rollup": "^1.29.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "standard": "^14.3.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.spec.ts"
    ]
  },
  "standard": {
    "ignore": [
      "node_modules/",
      "build/"
    ],
    "globals": [
      "test",
      "expect"
    ]
  }
}
